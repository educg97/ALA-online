{
  "name": "american-language-online",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "develop": "ng serve --host 0.0.0.0",
    "dev": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "deploy": "git pull origin master && supervisorctl restart web && ng build --prod && rm -rf prod && cp -r dist/ prod",
    "clean": "rm -rf dist",
    "minify": "uglifyjs dist/bundle.js --ie8 --compress --mangle --output dist/bundle.min.js",
    "build_prod": "npm run clean && npm run build && npm run minify",
    "test": "ng test",
    "lint": "ng lint",
    "backend": "node ./backend/server.js",
    "nodemon-backend": "nodemon ./backend/server.js",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "angular-google-recaptcha": "^1.0.3",
    "bluebird": "^3.5.5",
    "core-js": "^2.5.4",
    "device-detector-js": "^2.2.1",
    "imap": "^0.8.19",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mailparser": "^2.7.1",
